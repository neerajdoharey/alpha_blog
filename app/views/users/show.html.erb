<h1 class="text-center">Welcome to <%= @user.username %></h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4 text-center">
    <%= gravatar_for @user, size: 150 %>
  </div>
  <div class="col-md-12">
    <h4 class="text-center"><%= @user.username %>'s articles</h4>
  </div>
<div>
<div class=" row text-center">
  <div class="col-md-12">
    <%= will_paginate @user_articles %>
  </div>
</div>
<%= render "articles/article" , obj: @user_articles %>
<div class=" row text-center">
  <div class="col-md-12">
    <%= will_paginate @user_articles %>
  </div>
</div>
